{
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "ignorePatterns":[
      "dist",
      "src/**/*.js"
    ],
    "plugins": [
      "@typescript-eslint"
    ]
}